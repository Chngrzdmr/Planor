<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="15.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Add your build properties and items here -->
  <PropertyGroup>
    <OutputPath>bin\</OutputPath>
    <TargetFramework>net472</TargetFramework>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="**\*.cs" />
  </ItemGroup>
  <Target Name="Build">
    <!-- Build the project -->
    <MSBuild Projects="@(Compile)" Targets="Build" Properties="OutputPath=$(OutputPath);TargetFramework=$(TargetFramework)" />
  </Target>
</Project>
